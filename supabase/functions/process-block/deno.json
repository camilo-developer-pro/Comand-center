{
  "name": "process-block",
  "description": "Supabase Edge Function for processing blocks with entity extraction and embedding generation",
  "version": "1.0.0",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/async": "jsr:@std/async@^1.0.0",
    "@std/datetime": "jsr:@std/datetime@^1.0.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/log": "jsr:@std/log@^1.0.0",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.39.0",
    "zod": "npm:zod@^3.22.0"
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --watch index.ts",
    "start": "deno run --allow-net --allow-env --allow-read index.ts",
    "test": "deno test --allow-net --allow-env --allow-read",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check index.ts"
  },
  "compilerOptions": {
    "lib": ["deno.window"],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": ["ban-untagged-todo"],
      "exclude": ["no-unused-vars"]
    }
  },
  "fmt": {
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "test": {
    "include": ["**/*.test.ts", "**/*.spec.ts"],
    "exclude": ["**/node_modules"]
  },
  "exclude": [
    "**/node_modules",
    "**/.git",
    "**/.DS_Store",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}